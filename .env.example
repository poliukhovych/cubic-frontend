# Example environment variables for frontend development
# Copy this to .env.development and fill in your values

# API Base URL (backend)
VITE_API_BASE_URL=http://localhost:8000

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
VITE_GOOGLE_REDIRECT_URI=http://localhost:5173/auth/callback
VITE_GOOGLE_USE_CODE_FLOW=1

# Development Mode (1 = dev mode with fake auth, 0 = production)
VITE_DEV_AUTH=1

# Admin Credentials (for dev mode)
VITE_ADMIN_USERNAME=admin
VITE_ADMIN_PASSWORD=admin123

# Base Path
VITE_BASE_PATH=/
